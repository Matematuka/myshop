{
    "version": 2,
    "builds": [
      {
        "src": "myshop/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "myshop/frontend",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/frontend/",
        "dest": "/myshop/frontend/dist/$1"
      },
      {
        "src": "/static/",
        "dest": "/myshop/static/$1"
      },
      {
        "src": "/",
        "dest": "myshop/wsgi.py"
      }
    ]
  }
  