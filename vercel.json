{
  "version": 2,
  "builds": [
    {
      "src": "myshop/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "frontend",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myshop/wsgi.py"
    }
  ]
}
