
{
    "version": 2,
    "builds": [
      {
        "src": "myshop/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.12"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/frontend/(.*)",
        "dest": "myshop/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ],
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ]
  }
  